# Render ISS Image:
for j in range (0, 64, 1):
    for i in range (0, 128, 1):
        Display.pixel(i, j, (ISS_Image[j][i]))
            
# Display ISS Image:
Display.show()

# Generate WLAN Object:
wlan = network.WLAN(network.STA_IF)
wlan.active(True)
wlan.connect("WiFi_Name", "WiFi_Password")

# Wait for WiFi Connection:
while wlan.isconnected == False:
    print("Connecting....")
    time.sleep(1)

# WiFi Connection Aquired:
print("Connected")
time.sleep(5)
Display.fill(0)

