# Begin Program Loop:
while True:
        
    # Render Map Image:
    for j in range (0, 64, 1):
        for i in range (0, 128, 1):
            Display.pixel(i, j, (Map_Image[j][i]))
                
    # Display Map Image:
    Display.show()

    # Data Object:
    ISS_Position = urequests.get("http://api.open-notify.org/iss-now.json").json()
    ISS_Latitude = ISS_Position["iss_position"]["latitude"]
    ISS_Longitude = ISS_Position["iss_position"]["longitude"]

    # Print Data:
    print("")
    print("ISS Location Data:")
    print("Latitude: " + ISS_Latitude)
    print("Longitude: " + ISS_Longitude)

    # Map ISS GPS Data to OLED Display:
    X_Position = int((float(ISS_Longitude) + 180) * (128 / 360))
    Y_Position = int((float(ISS_Latitude) + 90) * (64 / 180))

    # Cross Hair in ISS Position:
    Display.line(0, Y_Position, 127, Y_Position, 1)
    Display.line(X_Position, 0, X_Position, 63, 1)
    Display.show()
    time.sleep(3)

